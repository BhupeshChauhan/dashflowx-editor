{
  "name": "@dashflowx/editor",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.cjs",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepack": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "commit": "yarn git-cz",
    "release:major": "npm version $(semver $npm_package_version -i major)",
    "release:minor": "npm version $(semver $npm_package_version -i minor)",
    "release:patch": "npm version $(semver $npm_package_version -i patch)"
  },
  "dependencies": {
    "@dashflowx/core": "^1.1.17",
    "@hookform/resolvers": "^3.6.0",
